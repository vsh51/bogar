<Application x:Class="Bogar.UI.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <SolidColorBrush x:Key="WindowBackgroundBrush" Color="White"/>
        <SolidColorBrush x:Key="LightSquareBrush" Color="#FFF8F5"/>
        <SolidColorBrush x:Key="DarkSquareBrush" Color="#808583"/>
        <SolidColorBrush x:Key="AccentBrush" Color="#2E86C1"/>
        <SolidColorBrush x:Key="TextBrush" Color="#222222"/>
        
        <BitmapImage x:Key="wP" UriSource="pack://application:,,,/Resources/Pieces/wP.png"/>
        <BitmapImage x:Key="wN" UriSource="pack://application:,,,/Resources/Pieces/wN.png"/>
        <BitmapImage x:Key="wB" UriSource="pack://application:,,,/Resources/Pieces/wB.png"/>
        <BitmapImage x:Key="wR" UriSource="pack://application:,,,/Resources/Pieces/wR.png"/>
        <BitmapImage x:Key="wQ" UriSource="pack://application:,,,/Resources/Pieces/wQ.png"/>
        <BitmapImage x:Key="wK" UriSource="pack://application:,,,/Resources/Pieces/wK.png"/>

        <BitmapImage x:Key="bP" UriSource="pack://application:,,,/Resources/Pieces/bP.png"/>
        <BitmapImage x:Key="bN" UriSource="pack://application:,,,/Resources/Pieces/bN.png"/>
        <BitmapImage x:Key="bB" UriSource="pack://application:,,,/Resources/Pieces/bB.png"/>
        <BitmapImage x:Key="bR" UriSource="pack://application:,,,/Resources/Pieces/bR.png"/>
        <BitmapImage x:Key="bQ" UriSource="pack://application:,,,/Resources/Pieces/bQ.png"/>
        <BitmapImage x:Key="bK" UriSource="pack://application:,,,/Resources/Pieces/bK.png"/>
    </Application.Resources>
</Application>
